{
  "settingsInheritedFrom": "rammdemocorp-mend/whitesource-config@main",
  "scanSettings": {
    "releaseBranches": ["^main$"]
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": true
    },
    "enableRenovate": true,
    "reviewers": ["ram-mend"],
    "prConcurrentLimit": 1,
    "packageRules": [
      {
        "matchPackageNames": ["*"],
        "commitMessagePrefix": "[JIRA-123]"
      }
    ],
    "extends": [
      "config:recommended",
      "mergeConfidence:all-badges",
      ":disableRateLimiting",
      ":reviewer('ram-mend')"
    ],
    "branchPrefix": "mend_update/"
  }
}

